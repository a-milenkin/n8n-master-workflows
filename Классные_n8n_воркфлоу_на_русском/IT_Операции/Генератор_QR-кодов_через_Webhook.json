      "parameters": {
        "path": "generate-qr",
        "options": {},
        "httpMethod": "POST",
        "responseMode": "responseNode"
      },
      "typeVersion": 2
    },
    {
      "id": "acd3ea0f-7694-4875-994c-3b365c10fc28",
      "name": "Respond with QR Code",
