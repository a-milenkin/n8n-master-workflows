      ],
      "parameters": {
        "content": "Обрабатывайте квартиры одну за другой для отправки заявок."
      },
      "typeVersion": 1
    },
    {
      "id": "sticky7",
      "name": "Sticky: Fetch Schufa",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        1450,
        30
      ],
      "parameters": {
        "content": "Получите отчет Schufa из Google Drive для прикрепления к заявке."
      },
      "typeVersion": 1
    },
    {
      "id": "sticky8",
      "name": "Sticky: Fetch Salary Slips",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        1450,
        430
      ],
      "parameters": {
        "content": "Получите последние платежные ведомости из Google Drive для заявки."
      },
      "typeVersion": 1
    },
    {
      "id": "sticky9",
      "name": "Sticky: Generate Cover Letter",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        1650,
