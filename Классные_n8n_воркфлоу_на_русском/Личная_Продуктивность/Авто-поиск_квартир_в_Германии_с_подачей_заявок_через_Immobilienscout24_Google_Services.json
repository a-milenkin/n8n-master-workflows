    },
    {
      "id": "sticky4",
      "name": "Sticky: Fetch Listings From immobilienscout24",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        850,
        230
      ],
      "parameters": {
        "content": "Получите списки квартир из ImmobilienScout24 с примененными фильтрами."
      },
