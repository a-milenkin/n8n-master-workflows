      ],
      "parameters": {
        "content": "Создайте персонализированное сопроводительное письмо с ID экспозиции и именем заявителя."
      },
      "typeVersion": 1
    },
    {
      "id": "sticky10",
      "name": "Sticky: Send Application Email",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        1850,
        230
      ],
      "parameters": {
        "content": "Отправьте электронное письмо с заявкой на квартиру с вложениями."
      },
      "typeVersion": 1
    },
    {
      "id": "sticky11",
      "name": "Sticky: Log to Google Sheet",
      "type": "n8n-nodes-base.stickyNote",
      "position": [
        2050,
        230
      ],
      "parameters": {
        "content": "Записывайте примененные квартиры в Google Sheets для отслеживания."
      },
      "typeVersion": 1
    }
  ],
  "active": true,
  "settings": {
    "timezone": "Europe/Berlin",
    "executionOrder": "regular",
    "saveDataOnSuccess": "allExecutions"
